{
  "name": "express-typescript-rest-api",
  "version": "1.0.0",
  "description": "Simple CRUD API boilerplate with Express and Typescript",
  "scripts": {
    "build": "rimraf dist && tsc",
    "build:watch": "rimraf dist && tsc -w",
    "dev": "nodemon --config nodemon.json src/index.ts",
    "lint": "eslint . --ext .ts,.js --fix",
    "test": "jest",
    "start": "npm run dev",
    "coverage": "jest test --coverage",
    "knex:seed:make": "knex seed:make --knexfile ./src/database/knexfile.ts -x ts",
    "knex:seed:run": "knex seed:run --knexfile ./src/database/knexfile.ts",
    "knex:migrate:make": "knex migrate:make --knexfile ./src/database/knexfile.ts -x ts",
    "knex:migrate:latest": "knex migrate:latest --knexfile ./src/database/knexfile.ts",
    "knex:migrate:rollback": "knex migrate:rollback --knexfile ./src/database/knexfile.ts"
  },
  "author": "louiswtf",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "date-fns": "^2.18.0",
    "dotenv": "^8.2.0",
    "email-templates": "^8.0.3",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.4.1",
    "indicative": "^7.4.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.18",
    "lodash": "^4.17.21",
    "mailgun-js": "^0.22.0",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "objection": "^2.2.14",
    "prettier": "^2.1.2",
    "pug": "^3.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.10",
    "@types/email-templates": "^8.0.2",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/faker": "^5.1.7",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.168",
    "@types/mailgun-js": "^0.22.11",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.8",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  }
}
